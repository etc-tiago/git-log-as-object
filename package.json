{
  "name": "git-log-as-object",
  "version": "0.1.2",
  "description": "Fetch the history for any commit range as a JavaScript object",
  "author": "smhxx <captaintrek@gmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "git",
    "log",
    "commit",
    "history",
    "json",
    "object"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/smhxx/git-log-as-object.git"
  },
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "coverage": "jest --coverage --colors",
    "deploy": "semantic-release",
    "lint": "tslint -p src",
    "lintspec": "tslint -p spec",
    "prepublishOnly": "npm run build",
    "test": "jest --colors"
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/git": "^2.0.2",
    "@types/jest": "^22.0.0",
    "@types/node": "^9.3.0",
    "jest": "^22.0.1",
    "semantic-release": "^12.2.2",
    "ts-jest": "^22.0.0",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.5.3"
  },
  "license": "MIT",
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "breaking": true,
          "release": "major"
        },
        {
          "type": "feat",
          "release": "minor"
        },
        {
          "type": "fix",
          "release": "patch"
        },
        {
          "type": "perf",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "docs",
          "scope": "README",
          "release": "patch"
        }
      ]
    },
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
