{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es6"
    ],
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../lib",
    "strict": true,
    "newLine": "lf",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "types": [ "node" ]
  }
}
